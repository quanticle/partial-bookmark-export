{
  "manifest_version": 2,
  "name": "Partial Bookmark Export",
  "version": "0.1",
  "description": "Export only some of your bookmarks, instead of all of them",
  "homepage_url": "https://github.com/quanticle/partial-bookmark-export",
  "icons": {
    "48": "icons/bookmark-48.png"
  },
  "permissions": [
    "bookmarks",
    "downloads"
  ],
  "browser_action": {
    "default_icon": "icons/bookmark-32.png",
    "default_title": "Partial bookmark export",
    "default_popup": "index.html"
  }
}